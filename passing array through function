void main()
{
  var a = [1, 2, "Ali", true];
  var b = [1, 2, "Ali", true];

  passArray(a);
  passArray(b);
}

passArray(list) {
  for (var x in list) {
    print(x);
  }
}

// OR

void main() {
  var a = [1, 2, "Ali", true];
  var b = [1, 2, "Ali", true];

  passArray2(a);
  passArray2(b);
}

passArray2(List list) {
  for (var i = 0; i < list.length; i++) {
    print("value: ${list[i]}");
  }
}
